{
  "expo": {
    "name": "target_app",
    "slug": "target_app",
    "scheme": "targetapp",
    "version": "1.0.0",
    "android": {
      "package": "com.target_app",
      "edgeToEdgeEnabled": true  ,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#3d44cd"
      }
    },
    "ios": {
      "bundleIdentifier": "com.target_app",
      "icon": "./assets/ios-icon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-sqlite",
      [
        "expo-system-ui",
        {
          "androidStatusBarBackgroundColor": "#ffffff",
          "androidNavigationBarBackgroundColor": "#ffffff"
        }
      ]
    ]
  }
}